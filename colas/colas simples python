class Cola:
    def __init__(self):
        self.arr = []

    def enqueue(self, x):
        self.arr.append(x)

    def dequeue(self):
        if self.arr:
            self.arr.pop(0)

    def mostrar(self):
        print(self.arr)

c = Cola()
c.enqueue(1)
c.enqueue(2)
c.enqueue(3)
c.mostrar()
c.dequeue()
c.mostrar()
