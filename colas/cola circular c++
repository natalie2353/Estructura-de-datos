#include <iostream>
using namespace std;

#define MAX 5

class ColaCircular {
    int arr[MAX], front, rear;
public:
    ColaCircular() { front = rear = -1; }

    void enqueue(int x) {
        if ((rear + 1) % MAX == front) return;
        if (front == -1) front = 0;
        rear = (rear + 1) % MAX;
        arr[rear] = x;
    }

    void dequeue() {
        if (front == -1) return;
        if (front == rear) front = rear = -1;
        else front = (front + 1) % MAX;
    }

    void mostrar() {
        if (front == -1) return;
        int i = front;
        while (true) {
            cout << arr[i] << " ";
            if (i == rear) break;
            i = (i + 1) % MAX;
        }
        cout << endl;
    }
};

int main() {
    ColaCircular c;
    c.enqueue(1);
    c.enqueue(2);
    c.enqueue(3);
    c.mostrar();
    c.dequeue();
    c.mostrar();
}
