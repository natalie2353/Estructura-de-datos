using System;

class BubbleSort {
    static void Main() {
        int[] arr = { 21, 7, 49, 28, 14, 1 };
        int n = arr.Length;

        for (int i = 0; i < n - 1; i++) {
            for (int j = 0; j < n - i - 1; j++) {
                if (arr[j] > arr[j + 1]) {
                    int temp = arr[j];
                    arr[j] = arr[j + 1];
                    arr[j + 1] = temp;
                }
            }
        }

        Console.WriteLine(string.Join(" ", arr));
    }
}