using System;

class Program {
    static void Main() {
        string[] lenguajes = {"Python", "Java", "C++", "JavaScript", "C#"};
        string buscado = "JavaScript";
        int indice = -1;

        for(int i = 0; i < lenguajes.Length; i++){
            if(lenguajes[i] == buscado){
                indice = i;
                break;
            }
        }

        Console.WriteLine(indice != -1 ? indice.ToString() : "No existe");
    }
}
