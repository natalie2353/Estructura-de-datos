#include <iostream>
using namespace std;

int main() {
    string lenguajes[] = {"Python", "Java", "C++", "JavaScript", "C#"};
    int n = 5;
    string buscado = "JavaScript";
    int indice = -1;

    for(int i = 0; i < n; i++){
        if(lenguajes[i] == buscado){
            indice = i;
            break;
        }
    }

    if(indice != -1)
        cout << "Ãndice: " << indice;
    else
        cout << "No existe";
}
